{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ],
  "installCommand": "cd ../.. && npm install",
  "buildCommand": "cd apps/api && npm run build",
  "crons": [
    {
      "path": "/cron/release-expired",
      "schedule": "0 0 * * *"
    }
  ]
}