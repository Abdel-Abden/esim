{
  "name": "ilotel",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "frontend": "npm run start --workspace=apps/frontend",
    "api": "npm run dev --workspace=apps/api",
    "build:shared": "npm run build --workspace=packages/shared",
    "lint": "npm run lint --workspaces --if-present",
    "typecheck": "npm run typecheck --workspaces --if-present"
  },
  "devDependencies": {
    "typescript": "~5.9.2"
  }
}